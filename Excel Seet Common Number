class Solution {
    public int titleToNumber(String columnTitle) {
        int n = columnTitle.length();
        int ans = 0;
        int base = 1;
        for (int i = n - 1; i >= 0; i--) {
            int temp = 0;
            if (columnTitle.charAt(i) == 'Z') {
                temp = 26;
            } else {
                temp = columnTitle.charAt(i) - 'A' + 1;
            }
            ans = ans + base * temp;
            base *= 26;
        }

        return ans;
    
    }
}
